<?xml version="1.0" encoding="utf-8"?>
<manifest name="com.gowild.voiceserver.xera">

    <!--
                              ereActivityManifest.xml样例
        **********************************需要注意**********************************
        1、每个应用只有一个启动EreActivity，所以多个#gowild.intent.action.LAUNCH 是不被允许的
        2、eraActivity标签对应的name值为EreActivity具体包名加类名，错误的类名不能被正确加载，重复类只会有最后一个生效
        3、action标签对应的name值为此EreActivity的属性，推荐使用action来启动EreActivity，这样扩展性可以有保证；
          action允许自定义，但是添加者有义务去确保action的唯一性；
          一个EreActivity可以对应多个action
        4、level标签代表着这个EreActivity的业务等级，高等级有更高的优先执行权，默认值为0
        5、其他标签不允许存在，不会被解析
    -->
    <eraActivity name="cn.gowild.lifeline.activity.HOMEEreActivity">
        <action name="gowild.intent.action.HOME"/>
        <level>0</level>
    </eraActivity>

    <eraActivity name="cn.gowild.bluetooth.BlueToothEreActivity">
        <action name="gowild.intent.action.BLUETOOTH"/>
        <level>0</level>
    </eraActivity>

    <eraActivity name="com.gowild.EreActivity.LaunchEreActivity">
        <action name="gowild.intent.action.LAUNCH"/>
        <level>10</level>
    </eraActivity>

    <eraActivity name="com.gowild.EreActivity.SettingsEreActivity">
        <action name="gowild.intent.action.SETTINGS"/>
        <level>0</level>
    </eraActivity>

    <eraActivity name="cn.gowild.settings.wifi.NetConfigurationActivity">
